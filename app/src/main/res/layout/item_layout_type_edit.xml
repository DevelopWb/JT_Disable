<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:paddingTop="3dp"
    android:paddingBottom="3dp"
    android:paddingLeft="15dp"
    android:paddingRight="15dp"
    android:layout_height="wrap_content">
    <EditText
        android:id="@+id/edit_value_et"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:background="@drawable/stroke_gray_square_bg"
        android:minHeight="26dp"
        android:paddingLeft="5dp"
        android:maxLength="11"
        android:paddingRight="5dp"
        android:textSize="14dp"
        app:layout_constraintLeft_toLeftOf="@+id/item_business_small_title_tv"
        app:layout_constraintTop_toBottomOf="@+id/item_business_small_title_tv"></EditText>

</android.support.constraint.ConstraintLayout>