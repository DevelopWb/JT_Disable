    </style>
    <style name="RCTheme.TextView" parent="android:Widget.TextView">

    </style>
    <declare-styleable name="AsyncImageView">

        <attr format="dimension" name="RCMinShortSideSize"/>
        <attr format="dimension" name="RCCornerRadius"/>
        <attr format="boolean" name="RCMask"/>

        <attr name="RCShape">
            <flag name="square" value="0"/>
            <flag name="circle" value="1"/>
        </attr>

        <attr format="reference" name="RCDefDrawable"/>

    </declare-styleable>
    <declare-styleable name="AutoLinkTextView">
        <attr format="dimension" name="RCMaxWidth"/>
    </declare-styleable>
    <declare-styleable name="EllipsizeTextView">
        <attr format="string|reference" name="RCEllipsizeText"/>
        <attr format="integer|reference" name="RCEllipsizeIndex"/>
    </declare-styleable>
    <declare-styleable name="RongExtension">
        <attr name="RCStyle">
            <enum name="SCE" value="0x123"/>
            <enum name="SC" value="0x120"/>
            <enum name="EC" value="0x320"/>
            <enum name="CE" value="0x023"/>
            <enum name="C" value="0x020"/>
        </attr>
    </declare-styleable>
